{
    "name": "pptr-mocha",
    "version": "1.0.0",
    "description": "Automated testing with Mocha and Puppeteer",
    "main": "index.js",
    "scripts": {
        "test": "node ./node_modules/mocha/bin/mocha --timeout=30000 ./build/tests/**.js",
        "// rm use in mac or linux":"",
        "// clean": "rm -rf build",
        "// del use in window":"",
        "clean": "cd scripts && rm-rf.bat",
        "build": "babel --preset es2015 -d build/ src/"
    },
    "keywords": [
        "Mocha",
        "Puppeteer",
        "Framework",
        "Automation"
    ],
    "author": "Viet Hoang",
    "license": "ISC",
    "dependencies": {
        "chai": "^4.3.4",
        "mocha": "^8.3.2",
        "mocha-steps": "^1.3.0",
        "puppeteer": "^8.0.0"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-preset-es2015": "^6.24.1"
    }
}
